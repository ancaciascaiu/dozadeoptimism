<%= form_for :user, url: users_path do |f| %>
  <%= f.label :superhero_name %>
  <%= f.text_field :superhero_name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit "Create" %>
<% end %>